{
  "name": "ng-toolbox",
  "version": "0.1.0",
  "description": "Set of Angular utilities used at Wizbii.",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "karma start --single-run",
    "build": "webpack && webpack -p -d",
    "dev": "webpack --progress --colors --watch",
    "commit": "npm run test && git-cz"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizbii/ng-toolbox.git"
  },
  "keywords": [
    "angular",
    "ng",
    "utilities",
    "toolbox",
    "utils"
  ],
  "author": "Wizbii <dev@wizbii.com> (http://www.wizbii.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wizbii/ng-toolbox/issues"
  },
  "homepage": "https://github.com/wizbii/ng-toolbox#readme",
  "devDependencies": {
    "angular": "^1.4.7",
    "angular-mocks": "^1.4.7",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "commitizen": "^2.0.2",
    "cz-conventional-changelog": "^1.0.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.11",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "pre-commit": "^1.1.1",
    "standard": "^5.4.1",
    "webpack": "^1.12.2"
  }
}
